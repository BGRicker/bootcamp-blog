<div class="article rows">
<% cache "#{article_small}_small" do %>
  <div class="article_rows">
    <h1><%= link_to article_small.title, article_path(article_small) %></h1>
    <h2><%= article_small.sub_title %></h2>
    <div class="author_name"><span>by</span> <%= display_article_authors(article_small) %> on <%= article_small.created_at.strftime('%A, %b %d') %></div></div>
<% end %>
</div>
